		<IsPackable>true</IsPackable>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="Client\**" />
	  <EmbeddedResource Remove="Client\**" />
	  <None Remove="Client\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Grpc.Net.Client" Version="2.38.0" />
